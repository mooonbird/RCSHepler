<Page x:Class="RCSHepler.AuthorizationInformationPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:RCSHepler"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="AuthorizationInformation">

    <StackPanel>
        <TextBlock FontFamily="MicrosoftYaHei" Foreground="#323232" FontSize="18px">License认证</TextBlock>
        <DockPanel  Margin="0,10,0,0" HorizontalAlignment="Left">
            <TextBlock VerticalAlignment="Center">授权状态：</TextBlock>
            <TextBlock VerticalAlignment="Center" Width="400">已授权</TextBlock>
            <Button Margin="10,0,0,0" Width="100" Style="{StaticResource button}" Click="Improt">导入</Button>
            <Button Margin="10,0,0,0" Width="100" Style="{StaticResource button}" Click="Export">导出</Button>
        </DockPanel>
        
    </StackPanel>
</Page>
