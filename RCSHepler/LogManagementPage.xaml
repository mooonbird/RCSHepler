<Page x:Class="RCSHepler.LogManagementPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:RCSHepler"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="LogManagementPage">

    <StackPanel VerticalAlignment="Top" Height="30" Orientation="Horizontal">
        <TextBlock FontFamily="MicrosoftYaHei" Foreground="#323232" FontSize="18px" VerticalAlignment="Center" Text="轮转周期"/>
        <ComboBox x:Name="comBox" SelectionChanged="ComboBox_SelectionChanged" Margin="30,0,0,0" Width="200"/>
        <Button Click="Button_Click" Width="60" Style="{StaticResource button}" Margin="30,0,0,0" Content="确 认"/>
    </StackPanel>
</Page>